Class Main inherits IO {
	x : Int <- 5;
	foo(z:Int) : Int {{
		out_int(x);
		out_string(",");
		out_int(z);
		out_string("\n");
		x+z;
	}};
	bar(y:Int) : Int {{
		let x : Int <- 1 in
		let z : Int <- 2 in
		foo(y);
	}};

	main(): Object {{
		let x : Int <- 7 in
		out_int(foo(bar(3)));
	}};
};
